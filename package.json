{
  "name": "create-nextjs-aptos-dapp-template",
  "version": "1.1.3",
  "description": "A generator for Next.js Aptos dApp projects",
  "main": "bin/generate.js",
  "type": "module",
  "bin": {
    "create-nextjs-aptos-dapp-template": "./bin/generate.js"
  },
  "files": [
    "template/**/*",
    "!template/node_modules",
    "!template/.next",
    "!template/bun.lock",
    "!template/package-lock.json",
    "!template/.env",
    "!template/.env.local",
    "!template/dist",
    "!template/build",
    "bin/generate.js",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/EATMove/Nextjs-Aptos-Dapp-Template"
  },
  "scripts": {
    "test": "node bin/generate.js --help || echo 'Generator script exists'",
    "test-local": "node bin/generate.js"
  },
  "keywords": [
    "nextjs",
    "aptos",
    "dapp",
    "template",
    "blockchain",
    "web3"
  ],
  "author": "DudeGuuud",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.4.1",
    "fs-extra": "^10.1.0",
    "inquirer": "^8.2.6"
  }
}
